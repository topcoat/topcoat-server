!!! 5
html
	head
		meta(charset = 'utf-8')
		meta('http-equiv' = 'X-UA-Compatible', content = 'IE=edge,chrome=1')
		title Topcoat Server
		meta(name="viewport", content="user-scalable=no,initial-scale = 1.0,maximum-scale = 1.0")
		link(rel = 'stylesheet', type = 'text/css', href = )
		link(rel = 'stylesheet', type = 'text/css', href = 'stylesheets/main.css')
		//if lt IE 9
			script(src = '//html5shiv.googlecode.com/svn/trunk/html5.js')
	body.light
		#wrapper
			.max-width
				#sideNav
					nav.site: ul
						li: a(href='http://topcoat.io') Home
						li: a(href='http://topcoat.io/topcoat') Usage Guidelines
						li.selected: a(href='#') Benchmarks
						li: a(href='http://topcoat.io/blog') Blog
						li: a#download-btn(href='https://github.com/topcoat/topcoat/archive/0.5.1.zip') Download version 0.5.1
					div.combo
						select.docNav
							option(value='#', selected="selected") Dashboard
							option(value='http://localhost:3000/view/results') Aggregated results
					#pageNav: ul#components
						include includes/components
			#site
				header#main-header
					.max-width
						hgroup
							h1 Topcoat Server
							p Results and benchmarks for Topcoat
						nav
							ul
								li: a(href='http://topcoat.io/topcoat') Usage Guidelines
								li.selected: a(href='#') Benchmarks
								li: a(href='http://topcoat.io/blog') Blog
								li: a#download-btn(href='https://github.com/topcoat/topcoat/archive/0.5.1.zip') Download version 0.5.1
						a#slide-menu-button.topcoat-icon-button--large--quiet
							span.topcoat-icon--large.topcoat-icon--menu-stack
				#content.max-width
					include includes/components-links
				footer

	script(src='/javascripts/libraries/jquery.min.js')
	script(src='/javascripts/libraries/jquery.flot.min.js')
	script(src='/javascripts/libraries/underscore-min.js')
	script(src='/javascripts/script.jplot.js')
	script(src='/javascripts/landingpage.js')
	script(src="/javascripts/libraries/d3.v3.min.js")
	script(src="/javascripts/baseline.js")
	script(src="/javascripts/main.js")